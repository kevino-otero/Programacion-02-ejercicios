public class NumerosImpares {
    public static void main(String[] args) {
        System.out.println("Números impares entre 1 y 50:");
        
        for (int i = 1; i <= 50; i++) {
            if (i % 2 != 0) {  // Verifica si el número es impar
                System.out.print(i + " ");
            }
        }
    }
}
